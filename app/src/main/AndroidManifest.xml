<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nightmarketunlocker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_nmu"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_nmu_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SigninActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchActivity" android:label="SearchActivity"></activity>
        <activity android:name=".SettingActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".Search2Activity" android:label="Search2Activity"/>
    </application>

</manifest>